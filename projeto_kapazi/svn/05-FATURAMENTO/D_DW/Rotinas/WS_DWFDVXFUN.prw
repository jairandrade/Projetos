User Function UnMaskCNPJ(cCNPJ)

Local cCNPJClear := ""

Default cCNPJ := ""

cCNPJClear := cCNPJ

Begin Sequence

	If Empty(cCNPJClear)
		Break
	EndIf

	cCNPJClear := StrTran(cCNPJClear,".","")
	cCNPJClear := StrTran(cCNPJClear,"/","")
	cCNPJClear := StrTran(cCNPJClear,"-","")

	cCNPJClear := AllTrim(cCNPJClear)
	
End Sequence
	
Return(cCNPJClear)

