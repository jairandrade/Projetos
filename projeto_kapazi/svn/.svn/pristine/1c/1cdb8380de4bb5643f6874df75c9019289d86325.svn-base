					cAgePa := E5_AGENCIA
					cCtaPa := E5_CONTA
					cChqPa := E5_NUMCHEQ
					Exit
				Endif		
				dbSkip()
			Enddo
		Endif	
	Endif		
	RestArea(aArea)
