User Function MA103BUT() 
Local aRet	:= {} 	//Retorno da função
	
	//Adiciona o botão
  Aadd(aRet, {"NOTE", {||U_KCOMA001(.T., "N")}, "Justificativa"})
  Aadd(aRet,{ "Valida Operações",{||U_GATil()}, "Valida Operações"})
  Aadd(aRet,{ "Replica ARMAZEM",{||U_CargaARM()}, "Replica ARMAZEM"})
  Aadd(aRet,{ "Replica C.Custo",{||U_CARGACC()}, "Replica C.Custo"})
  
Return aRet
