	@ nLin,00 psay cDet
 	nLin ++
 	//CAMPO 36 Capital Atribuido a este Estabelecimento 
	@ nLin,00 psay STR0036 
	nLin ++
	cDet := "|" + space(26)+Transform(nCapEstb,"@E 999,999,999.99") +space(1)
