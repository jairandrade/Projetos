		If( !Empty(nQtd), AllTrim(Transform(nQtd,"@E 999,999,999")), nQtd := "")
		//Valor
		If ( !Empty(nVal), AllTrim(Transform(nVal,"@E 999,999,999.99")), nVal := "" )
