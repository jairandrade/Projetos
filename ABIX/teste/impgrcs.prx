		cDet += "X"+substr(STR0023,13,30)		 
	ElseIf cTpEstab =="04"
		cDet += space(4) + substr(STR0023,1,11)
		cDet += substr(STR0023,12,10)+"X"+substr(STR0023,23,20)
	ElseIf cTpEstab =="01" .or. cTpEstab =="02"
		cDet += space(4)+substr(STR0023,1,43)	
	Endif 
